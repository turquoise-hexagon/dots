# ---
# ncmpcpp bindings
# ---

def_key "h"
    previous_column

def_key "j"
    scroll_down

def_key "k"
    scroll_up

def_key "l"
    next_column

def_key ":"
    execute_command

def_key "/"
    find

def_key "space"
    pause

def_key "a"
    add_item_to_playlist

def_key "left"
    seek_backward

def_key "right"
    seek_forward
